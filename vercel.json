{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "250mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "memory": 3008,
            "maxDuration": 60
        }
    },
    "env": {
        "PYTHONPATH": ".",
        "PYTHONUNBUFFERED": "1",
        "PYTHONOPTIMIZE": "2"
    }
}
