{
    "version": 2,
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "10mb",
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/app.py"
        }
    ]
}
