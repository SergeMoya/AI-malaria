{
    "version": 2,
    "builds": [
        {
            "src": "frontend/**",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "frontend/build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}
